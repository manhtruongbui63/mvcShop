@model List<Model.EF.Menu>
<div class="wrap" style="position:relative">
    <div class="logo" style="max-width:7%">
        <a href="/"><img src="/assets/client/images/logopnj.png" alt="" /></a>
    </div>
    <div class="cssmenu" style="position:absolute; right:0">
        <ul>
            @foreach (var item in Model)
            {
                <li><a href="@item.Link">@item.Text</a></li>
            }
        </ul>
    </div>
    <div class="clear"></div>
</div>
